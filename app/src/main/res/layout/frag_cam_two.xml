<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CamTwoFrag">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <Button
            android:id="@+id/expandableButtoncn1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ba1f04"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtoncn1"
            android:paddingRight="10dp"
            android:text="LENS"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutcn1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#90ba1f04"
            android:orientation="vertical"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/f2t1"
                android:textStyle="bold"
                android:text="Nikon AF 70-300mm f/4-5.6G Telephoto Zoom Lens for Nikon DSLR Camera" />
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t2"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Nikon AF-S DX NIKKOR 55-200MM F/4-5.6G ED VR II"/>
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t3"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Nikon AF-S DX Nikkor 35mm f/1.8G Prime Lens for Nikon Digital SLR Camera (Black)"/>
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t4"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Nikon AF-S ED Nikkor 14-24mm F/2.8 G Zoom Lens for Nikon DSLR Camera"/>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/recycleviewcnl"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>


        <Button
            android:id="@+id/expandableButtoncn2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#b77803"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtoncn2"
            android:paddingRight="10dp"
            android:text="BATTERY"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutcn2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#90b77803"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="false"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Nikon NB-6LH Battery Pack" />

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Nikon NB-8L Li-Ion Battery Pack for Canon A3100IS and A3000IS Digital Cameras - Retail Packaging"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Nikon LP-E6 Original battery" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Nikon LP-E6 Battery Pack for Select Canon Digital SLR Cameras - Retail Packaging" />

        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

        <Button
            android:id="@+id/expandableButtoncn3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#10a106"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtoncn3"
            android:paddingRight="10dp"
            android:text="TRIPOD"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutcn3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#9010a106"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="false"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Smiledrive Expandable Long Universal Mobile Tripod - 360 Degrees Rotatable"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Photron Tripod Stedy 400 with Pan Head 4.5 Feet + Carry Case"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Nikon D3300 DSLR Camera (Body with AF-P DX NIKKOR 18 - 55 mm F3.5 - 5.6 VR Kit Lens)  (Black)"/>

        </com.github.aakira.expandablelayout.ExpandableLinearLayout>


        <Button
            android:id="@+id/expandableButtoncn4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#066da1"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtoncn4"
            android:paddingRight="10dp"
            android:text="HOUSING"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutcn4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#90066da1"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="false"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Case Logic DCB-305"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="20M Underwater Waterproof Housing Bag Case DSLR SLR For Canon Nikon video Camera"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Nikon EOS 7D DSLR Camera (Body only)  (Black)"/>

        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

    </LinearLayout>
</android.support.v4.widget.NestedScrollView>