<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LapFiveFrag">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <Button
            android:id="@+id/expandableButtonlaa1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ba1f04"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlaa1"
            android:paddingRight="10dp"
            android:text="SCREEN"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlaa1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#90ba1f04"
            android:orientation="vertical"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/f2t1"
                android:textStyle="bold"
                android:text="ASUS G750JS LCD" />
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t2"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS G750JW LCD"/>
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t3"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS ROG GEN3 LCD"/>
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t4"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS ROG GEN4 LCD"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>


        <Button
            android:id="@+id/expandableButtonlaa2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#b77803"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlaa2"
            android:paddingRight="10dp"
            android:text="KEYBOARD"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlaa2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#90b77803"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS REGA K53SJ KEYBOARD" />

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS REGA K53SW KEYBOARD"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS ROG K53U KEYBOARD"/>

        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

        <Button
            android:id="@+id/expandableButtonlaa3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#10a106"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlaa3"
            android:paddingRight="10dp"
            android:text="MOTHERBOARD"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlaa3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#9010a106"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS G41 TMLX-2 MotherBoard"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUSASUS H170 PRO MotherBoard"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS M5A78L-MLX3 MotherBoard"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>


        <Button
            android:id="@+id/expandableButtonlaa4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#066da1"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlaa4"
            android:paddingRight="10dp"
            android:text="BATTERY"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlaa4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#90066da1"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS K43 SERIES Battery"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS A41-X5650 Battery"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS A41-X550 Battery"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

        <Button
            android:id="@+id/expandableButtonlaa5"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/rr"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlaa5"
            android:paddingRight="10dp"
            android:text="RAM"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlaa5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/rrr"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS 2GB RAM DDR3L"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS 4GB DDR3-1600"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS 8GB DDR3-1600 "/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

        <Button
            android:id="@+id/expandableButtonlaa6"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/bb"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlaa6"
            android:paddingRight="10dp"
            android:text="HARD DRIVES"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlaa6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/bbb"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS X551CA Hard Drive"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS NoteBook X550J Hard Drive"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS N550JV Hard Drive"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="ASUS NoteBookA53E Hard Drive"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>


    </LinearLayout>
</android.support.v4.widget.NestedScrollView>
