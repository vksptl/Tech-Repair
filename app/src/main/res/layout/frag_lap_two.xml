<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TwoFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <Button
            android:id="@+id/expandableButtonld1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ba1f04"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonld1"
            android:paddingRight="10dp"
            android:text="SCREEN"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutld1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#90ba1f04"
            android:orientation="vertical"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/f2t1"
                android:textStyle="bold"
                android:text="Dell Inspiron 1545 Display " />
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t2"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell ChromeBook 13 GEN2 LCD"/>
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t3"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="DEll ChromeBook 15 GEN2 LCD"/>
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t4"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell XPS 18 LCD"/>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/recycleviewls"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

        </com.github.aakira.expandablelayout.ExpandableLinearLayout>


        <Button
            android:id="@+id/expandableButtonld2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#b77803"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonld2"
            android:paddingRight="10dp"
            android:text="KEYBOARD"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutld2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#90b77803"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell Inspiron 1545 KEYBOARD" />

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Inspiron 15 N5050 KEYBOARD"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Inspiron 14R KEYBOARD"/>

        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

        <Button
            android:id="@+id/expandableButtonld3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#10a106"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonld3"
            android:paddingRight="10dp"
            android:text="MOTHERBOARD"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutld3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#9010a106"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="DEll XPS 18 MotherBoard"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="DEll XPS 15 MotherBoard"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell Inspiron 1545 MotherBoard"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>


        <Button
            android:id="@+id/expandableButtonld4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#066da1"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonld4"
            android:paddingRight="10dp"
            android:text="BATTERY"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutld4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#90066da1"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell Inspiron 5372 Battery"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell Inspiron 1525 Battery"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell Inspiron 1545 Battery"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

        <Button
            android:id="@+id/expandableButtonld5"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/rr"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonld5"
            android:paddingRight="10dp"
            android:text="RAM"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutld5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/rrr"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="DEll 2GB DDR3-1600 UDIMM"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="DEll 4GB DDR3-1600 UDIMM"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="DEll *GB DDR3-1600 SODIMM "/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

        <Button
            android:id="@+id/expandableButtonld6"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/bb"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonld6"
            android:paddingRight="10dp"
            android:text="HARD DRIVES"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutld6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/bbb"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell Inspiron n5050 Hard Drive"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell Inspiron 15-3521 1TB Hard Drive"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell 800GB XS1715 Hard Drive"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="Dell 1TB XS1715 Hard Drive"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

    </LinearLayout>
</android.support.v4.widget.NestedScrollView>