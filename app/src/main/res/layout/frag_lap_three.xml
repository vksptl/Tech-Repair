<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LapThreeFrag">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <Button
            android:id="@+id/expandableButtonlh1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ba1f04"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlh1"
            android:paddingRight="10dp"
            android:text="SCREEN"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlh1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#90ba1f04"
            android:orientation="vertical"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/f2t1"
                android:textStyle="bold"
                android:text="HP ChromeBook 11 G2 LCD " />
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t2"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP ChromeBook 11 G4 LCD"/>
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t3"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="LCD N156B6-L0B"/>
            <TextView
                android:layout_width="match_parent"
                android:id="@+id/f2t4"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP XPS G5 LCD"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>


        <Button
            android:id="@+id/expandableButtonlh2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#b77803"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlh2"
            android:paddingRight="10dp"
            android:text="KEYBOARD"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlh2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#90b77803"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP Cpompaq 431 KEYBOARD" />

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP Compaq 435 KEYBOARD"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP Compaq Cq43 KEYBOARD"/>

        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

        <Button
            android:id="@+id/expandableButtonlh3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#10a106"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlh3"
            android:paddingRight="10dp"
            android:text="MOTHERBOARD"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlh3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#9010a106"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP Pavilion DV 2000 MotherBoard"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP Pavilion DV 3000 MotherBoard"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP Pavilion DV 4000 MotherBoard"/>
            <android.support.v7.widget.RecyclerView
                android:id="@+id/recycleviewlm"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>


        <Button
            android:id="@+id/expandableButtonlh4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#066da1"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlh4"
            android:paddingRight="10dp"
            android:text="BATTERY"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlh4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#90066da1"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP EliteBook 2560p Battery"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP Pavilion 6720T Battery"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP Pavilion 8710W Battery"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

        <Button
            android:id="@+id/expandableButtonlh5"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/rr"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlh5"
            android:paddingRight="10dp"
            android:text="RAM"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlh5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/rrr"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="DEll 2GB DDR3-1600 UDIMM"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP 4GB DDR3L RAM"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP 8GB RAM PC3L "/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

        <Button
            android:id="@+id/expandableButtonlh6"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/bb"
            android:drawableRight="@android:drawable/arrow_down_float"
            android:onClick="expandableButtonlh6"
            android:paddingRight="10dp"
            android:text="HARD DRIVES"
            android:textColor="#fff" />

        <com.github.aakira.expandablelayout.ExpandableLinearLayout
            android:id="@+id/expandableLayoutlh6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/bbb"
            android:padding="16dp"
            app:ael_duration="400"
            app:ael_expanded="true"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP 750GB 7200rpm Hard Drive"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP 1TB Hard Drive"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP 500GB DV6531 Hard Drive"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_marginTop="10dp"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:text="HP 1TB DV6531 Hard Drive"/>
        </com.github.aakira.expandablelayout.ExpandableLinearLayout>

    </LinearLayout>
</android.support.v4.widget.NestedScrollView>